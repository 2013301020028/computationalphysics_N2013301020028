#  problem 1
from pylab import *
import pickle
v=[0] 
t=[0] 
g=9.8
print"dt:"
dt=float(raw_input())
print"end_time:"
end_time=float(raw_input())
for i in range(int(end_time/dt)):
        tmp=v[i]-g*dt
        v.append(tmp) 
        t.append(dt*(i+1))
        print t[-1],v[-1]
plot(t,v,'k')
show()
